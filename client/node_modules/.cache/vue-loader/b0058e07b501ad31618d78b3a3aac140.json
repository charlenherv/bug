class=\"PlaylistBox_Controls\" v-if=\"$store.state.currentUser && $store.state.currentUser._id === item.userId\">\n      <div @click=\"deletePlaylist\" data-button=\"grey\">Delete</div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Vue from 'vue';\nimport axios from \"axios\";\nimport AudioBox from \"../audio/AudioBox.vue\"\nimport { AudioFunctions } from '../../mixins/AudioFunctions.js';\nexport default {\n  name: \"PlaylistBox\",\n  components: {\n    AudioBox\n  },\n  mixins: [AudioFunctions],\n  props: ['appData','item','mode'],\n  data() {\n    return {\n      loaded: false,\n      view: 'initial'\n    }\n  },\n  computed: {\n    soundsCount: function(){\n      return this.item.sounds && this.item.sounds.length > 0 ? this.item.sounds.length + ' sounds': 'No sounds'\n    },\n    sounds: function(){\n      return []\n    }\n  },\n  methods: {\n    toggleView() {\n      const pBox = this;\n      const view = pBox.view === 'initial' ? 'open' : 'initial';\n      const sounds = this.item.sounds;\n      if (sounds && sounds.length > 0 && view === 'open' && pBox.loaded === false) {\n        axios\n        .get('/getSounds', { params: { sounds: this.item.sounds } })\n        .then(function(response){\n          pBox.$emit('sounds', response.data.sounds);\n          pBox.loaded = true;\n        })\n      }\n      this.view = view;\n    },\n    deletePlaylist() {\n      const comp = this;\n      axios\n        .post('/deletePlaylist', {}, { params: { playlistId: this.item._id } })\n        .then(function(response){\n          comp.$store.dispatch('processAndSetUser', 'refresh');\n        })\n    }\n  },\n  created() {\n    \n      \n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.PlaylistBox {\n  cursor: pointer;\n  position: relative;\n  float: left;\n  vertical-align: top;\n  padding: 25px 10px 20px 10px;\n  border: 1px solid grey;\n  margin: 10px;\n  text-align: left;\n  border-radius: $border-radius;\n  .PlaylistBox_Header {\n    text-transform: uppercase;\n    font-weight: bold;\n    font-size: 1.15em;\n  }\n  &[data-view=\"open\"] {\n    float: none;\n    clear: both;\n  }\n}\n</style>\n"]}]}