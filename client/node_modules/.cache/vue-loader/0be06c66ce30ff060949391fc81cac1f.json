|| 'initial'\"\n            :contenteditable=\" item.status === 'complete' ? 'false': 'true' \"\n            spellcheck=\"false\"\n            @keydown=\"keyDownFunctions($event,item.callback)\"\n        >{{item.defaultValue}}</div>\n      </div>\n  </div>\n</template>\n\n<script>\nimport Vue from 'vue'\nimport { closable } from '../../directive/closable.js';\nVue.directive('closable', closable);\n\nexport default {\n        name: 'SuperInput',\n        props: ['appData'],\n        methods: {\n            onClose: function(){\n                Vue.set(this.appData.tools.superInput,'state', false);\n            },\n            keyDownFunctions: function(event,callback){\n                var CropperData = this;\n                var type = event.target.getAttribute('data-type');\n                var value = event.target.innerText;\n\n                // General filter\n\n                // Options\n                \n                // Single-line\n                if (type === 'singleLine') {\n                    if (event.which === 13) {\n                        event.preventDefault();\n\n                        // Callback\n                        callback(value, function() {\n                            Vue.set(CropperData.appData.tools.superInput,'state', false);\n                        });\n                        \n                    }\n                }\n            }\n        },\n        data() {\n            return {\n                position: {\n                    top: 0,\n                    left: 0\n                }\n            }\n        },\n        mounted(){\n            this.position = {\n                left: this.appData.shared.mousePosition.left + 'px',\n                top: this.appData.shared.mousePosition.top + 'px'\n            }\n\n            // Focus and select all text\n            this.$refs.formItem[0].focus();\n            document.execCommand('selectAll', false, null)\n        }\n    }\n</script>\n<style lang=\"scss\">\n    .SuperForm {\n        z-index: 1000;\n        position: absolute;\n        padding: 10px;\n        box-sizing: border-box;\n        background: transparent;\n        .SuperForm_Inner {\n            padding: 5px;\n            box-sizing: border-box;\n            background: rgb(26, 74, 145);;\n            .SuperForm_Item {\n                min-height: 30px;\n                min-width: 250px;\n                padding: 10px;\n                margin-bottom: 10px;\n                box-sizing: border-box;\n                background: white;\n                font-size: 100%;\n                font-weight: 600;\n                line-height: 100%;\n                color: black;\n                outline: none;\n                &[data-type=\"singleLine\"] {\n\n                }\n                &[data-type=\"multiLine\"] {\n\n                }\n                &:last-child {\n                    margin-bottom: 0;\n                }\n            }\n        }\n    }\n</style>\n"]}]}