,IAAA,CAAA,QAAA,CAAA,IAAA,IAAA,KAAA;AACA;AACA,KAnCA;AAoCA,IAAA,WApCA,yBAoCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,QADA,GACA,IAAA,QAAA,EADA;AAEA,gBAAA,QAAA,CAAA,MAAA,CAAA,OAAA,EAAA,IAAA,CAAA,SAAA,CAAA,KAAA,CAAA,IAAA,CAAA;AACA,gBAAA,KAAA,CAAA,IAAA,CAAA,cAAA,EAAA,QAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,QAAA;;AACA,kBAAA,KAAA,CAAA,KAAA,CAAA,aAAA,EAAA,KAAA,CAAA,IAAA,CAAA,GAAA;AACA,iBAJA,EAKA,KALA,CAKA,UAAA,GAAA,EAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,iBAPA;;AAHA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA,KA/CA;AAiDA;AACA,IAAA,UAlDA,wBAkDA;AACA,WAAA,KAAA,CAAA,WAAA,GAAA,IAAA;AACA,UAAA,OAAA,GAAA,KAAA,IAAA,CAAA,GAAA;AACA,UAAA,YAAA,GAAA,KAAA,OAAA,GAAA,QAAA,GAAA,MAAA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,YAAA,EAAA;AACA,QAAA,YAAA,EAAA,YADA;AAEA,QAAA,OAAA,EAAA;AAFA,OAAA;AAIA,KA1DA;AA4DA;AACA,IAAA,qBA7DA,mCA6DA;AACA,WAAA,cAAA,GAAA,KAAA,cAAA,KAAA,CAAA,GAAA,CAAA,GAAA,CAAA;AACA,KA/DA;AAgEA,IAAA,kBAhEA,8BAgEA,UAhEA,EAgEA;AACA,UAAA,OAAA,GAAA,KAAA,IAAA,CAAA,GAAA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,oBAAA,EAAA;AACA,QAAA,OAAA,EAAA,OADA;AAEA,QAAA,UAAA,EAAA;AAFA,OAAA;AAIA,KAtEA;AAuEA,IAAA,mBAvEA,+BAuEA,KAvEA,EAuEA;AACA,UAAA,KAAA,GAAA,KAAA,CAAA,MAAA,CAAA,SAAA;AACA,WAAA,aAAA,GAAA,KAAA;AACA,KA1EA;AA2EA,IAAA,WA3EA,yBA2EA;AACA,WAAA,cAAA,GAAA,CAAA;AACA,KA7EA;AA8EA,IAAA,cA9EA,4BA8EA;AACA,UAAA,YAAA,GAAA,IAAA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,gBAAA,EAAA;AACA,QAAA,OAAA,EAAA,KAAA,IAAA,CAAA,GADA;AAEA,QAAA,aAAA,EAAA,KAAA,aAFA;AAGA,QAAA,QAAA,EAAA,oBAAA;AACA,UAAA,YAAA,CAAA,cAAA,GAAA,CAAA;AACA;AALA,OAAA;AAQA,WAAA,aAAA,GAAA,EAAA;AACA;AAzFA,GAlCA;AA6HA,EAAA,OA7HA,qBA6HA;AACA,SAAA,IAAA,CAAA,IAAA,GAAA,KAAA,IAAA,CADA,CACA;AACA;AA/HA,CAAA","sourcesContent":["<template>\n  <div :class=\"{\n      AudioBox_Container: true,\n      AudioBox_Expanded: currentlyPlaying || userLoggedIn && isLiked || userLoggedIn && likeUpdated\n    }\">\n    <div class=\"AudioBox_Details\" style=\"display:none\">\n        <div class=\"AudioBox_Title\" :contenteditable=\"mode === 'edit' ? true: false\" spellcheck=\"false\" @blur=\"event => editableUnfocus(event,'title')\">{{item.editable.title}}</div>\n        <div class=\"AudioBox_Author\">{{item.author}}</div>\n    </div>\n    <div \n      :data-status=\"item.status\"\n      :data-mode=\"mode\"\n      :data-color=\"item.color || 'grey'\"\n      :data-animation=\"item.editable.effects ? item.editable.effects.join(): ''\"\n      :class=\"{\n      'AudioBox':true,\n      \n        // EFFECTS: Classes and Attributes\n        'midBass':(activateEffects && appData.currentItem.midBass === true && appData.currentItem.deepBass !== true), \n        'deepBass':(activateEffects && appData.currentItem.deepBass === true),\n        'treble':(activateEffects && appData.currentItem.treble === true)}\" \n        :data-step=\"activateEffects && appData.currentItem.bassSteps && appData.currentItem.bassSteps.current ? appData.currentItem.bassSteps.current:0\"\n      >  \n\n      <!-- EFFECTS: Display elements-->\n        <!-- EQ background-->\n        <div class=\"AudioBox_EQ\" \n          v-if=\"activateEffects && appData.currentItem.levels\" \n          :style=\"{ opacity: appData.effects.power === true && appData.currentItem._id === item._id ? 1:0 }\"\n        >\n          <!-- Frequency level bocks-->\n          <div v-for=\"(level,index) in appData.currentItem.levels.perc\" :key=\"index\" :data-level=\"index\" :style=\"{opacity: appData.currentItem.levels.perc[index] >= 80 ? appData.currentItem.levels.perc[index] * 0.01 : 0}\"></div>\n        </div>\n\n      <!-- MAIN -->\n      <div id=\"main\" class=\"AudioBox_Section\">\n        <div class=\"AudioBox_Row\" data-display=\"flex\">\n          <div class=\"AudioBox_Column AudioBox_Main\">\n\n\n            <!-- Player and controls-->\n            <div class=\"AudioBox_Player\">\n              <div class=\"AudioBox_PlayerControls\">\n                <div class=\"AudioBox_PlayerCube\" @click=\"playSound(appData, item, {playMode: mode})\"></div>\n              </div>\n            </div>\n\n            <!-- Sound information-->\n            <div class=\"AudioBox_Details\">\n              <div class=\"AudioBox_Title\" :contenteditable=\"mode === 'edit' ? true: false\" spellcheck=\"false\" @blur=\"event => editableUnfocus(event,'title')\">{{item.editable.title}}</div>\n              <div class=\"AudioBox_Author\">{{item.author}}</div>\n            </div>\n          </div>\n\n          <!-- Marketplace --> \n          <div class=\"AudioBox_Column AudioBox_Tools\">\n              <div class=\"AudioBox_Delete\" data-background=\"grey\" v-if=\"mode === 'edit'\" @click=\"deleteSound\">Delete</div>\n              <div class=\"AudioBox_Buy\" data-background=\"green\" v-if=\"item.editable.marketplace && item.editable.marketplace.mode === 'forSale' && item.editable.marketplace.price\">Buy | <span class=\"AudioBox_BuyCurrency\">$</span><span class=\"AudioBox_BuyPrice\">{{item.editable.marketplace.price}}</span></div>\n              <div class=\"AudioBox_Buy\" data-background=\"blue\" v-if=\"item.editable.marketplace && item.editable.marketplace.mode === 'freeDownload'\">Download</div>\n          </div>\n\n        </div>\n      </div>\n      \n      <!-- UPLOAD -->\n      <div v-if=\"mode === 'upload' || mode === 'edit'\">\n\n        <!-- Trimmer -->\n        <div class=\"AudioBox_Section\">\n          <div class=\"AudioBox_Row\" v-if=\"mode === 'upload' || mode === 'edit'\">\n              <div class=\"AudioBox_AudioEditing\">\n                <AudioTrimmer :item=\"item\" @cropData=\"updateTrim\" :appData=\"appData\"></AudioTrimmer>\n              </div>\n          </div>\n        </div> \n\n        <!-- Description -->\n        <div class=\"AudioBox_Section\" v-if=\"item.editable.description\">\n          <div class=\"AudioBox_Row\">\n            <div class=\"AudioBox_Description\" contenteditable=\"true\" spellcheck=\"false\" @blur=\"event => editableUnfocus(event,'description')\">{{item.editable.description}}</div>\n          </div>\n        </div>  \n\n        <!-- Visual FX -->\n        <div class=\"AudioBox_Section\">\n          <div class=\"AudioBox_SectionHeader1\">Tags</div>\n          <div class=\"AudioBox_Row\">\n            <div class=\"AudioBox_Tags\" v-for=\"(tag,index) in appData.itemSettings.tags\" :key=\"index\" :data-button=\"item.editable.tags && item.editable.tags.includes(tag.id) ? 'on': 'off'\" :data-button-color=\"'grey, blue'\" @click=\"toggleTag(tag.id)\">{{tag.label}}</div>\n          </div>\n        </div>\n\n        <!-- Marketplace -->\n        <div class=\"AudioBox_Section\">\n\n          <div class=\"AudioBox_SectionHeader1\">Marketplace</div>\n          <div class=\"AudioBox_Row\">\n            <div class=\"AudioBox_MarketplaceMode\" v-for=\"(mode,index) in appData.itemSettings.marketplace.mode\" :key=\"index\" :data-mode=\"mode.id\" :data-button=\"item.editable.marketplace.mode === mode.id ? 'on': 'off' \" :data-button-color=\"'grey,'+mode.color\" @click=\"changeItemSetting(item.editable.marketplace,'mode',mode.id)\">{{mode.label}}</div>\n          </div>\n\n          <div class=\"AudioBox_Row\" v-if=\"item.editable.marketplace.mode === 'forSale'\">\n            <div class=\"AudioBox_SectionHeader2\">Price</div>\n            <div class=\"AudioBox_MarketplacePrice\" data-background=\"green\">\n              <input v-model=\"item.editable.marketplace.price\"/>\n            </div>\n          </div>\n\n          <div class=\"AudioBox_Row\" v-if=\"item.editable.marketplace.mode === 'forSale'\">\n            <div class=\"AudioBox_SectionHeader2\">Contract</div>\n            \n          </div>\n        </div>\n\n        <!-- Visual FX -->\n        <div class=\"AudioBox_Section\">\n          <div class=\"AudioBox_SectionHeader1\">Visual FX</div>\n          <div class=\"VisualEffects_Selection\">\n            <div @click=\"toggleEffect('bass')\" data-effect=\"bass\" :class=\"{'currentSelection': item.editable.effects && item.editable.effects.includes('bass')}\"></div>\n            <div @click=\"toggleEffect('colors-steps')\" data-effect=\"colors-steps\" :class=\"{'currentSelection': item.editable.effects && item.editable.effects.includes('colors-steps')}\"\n            ></div>\n            <div @click=\"toggleEffect('genesis9-cubes')\" data-effect=\"genesis9-cubes\"\n            :class=\"{'currentSelection': item.editable.effects && item.editable.effects.includes('genesis9-cubes')}\"\n            >\n              <div data-background=\"red\"></div>\n              <div data-background=\"orange\"></div>\n              <div data-background=\"yellow\"></div>\n              <div data-background=\"green\"></div>\n              <div data-background=\"blue\"></div>\n              <div data-background=\"grey\"></div>\n              <div data-background=\"indigo\"></div>\n              <div data-background=\"violet\"></div>\n              <div data-background=\"red\"></div>\n            </div>\n\n          </div>\n          </div>\n        </div>\n\n    </div>\n    <div \n    v-if=\"currentlyPlaying || userLoggedIn && isLiked || userLoggedIn && likeUpdated\" \n    class=\"UserInteractions\" style=\"float:right\">\n      <div :class=\"{ UserInteraction: true, Liked: isLiked }\" @click=\"updateLike()\"></div>\n\n      <div class=\"UserInteraction\" @click=\"togglePlaylistOptions()\"></div>\n      <div class=\"PlaylistModal\" v-if=\"playlistScreen !== 0\">\n        <div class=\"PlayListModal_Header\">Add To</div>\n        <div class=\"PlaylistModal_View1\" v-if=\"playlistScreen === 1\">\n          <div class=\"PlayListModal_Body\" contenteditable=\"false\">\n            <div v-for=\"(playlist,index) in userPlaylists\" :key=\"index\" @click=\"updatePlaylistItem(playlist._id)\" \n            :class=\"{\n              PlaylistModal_Playlist: true,\n              Active: playlist.sounds.includes(item._id)\n            }\">{{playlist.title}}</div>\n            <div class=\"PlaylistModal_AddPlaylist\" @click=\"addPlaylist()\">+ Create new playlist</div>\n          </div>\n        </div>\n        <div class=\"PlaylistModal_View2\" v-if=\"playlistScreen === 2\">\n          <div class=\"PlayListCreate_Title\" contenteditable=\"true\" spellcheck=\"false\" @blur=\"event => createPlaylistTitle(event)\">{{playlistTitle}}</div>\n          <button @click=\"createPlaylist()\">Create</button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Vue from 'vue';\nimport axios from \"axios\";\nimport { AudioFunctions } from '../../mixins/AudioFunctions.js'\nimport { UploadFunctions } from '../../mixins/UploadFunctions.js'\nimport AudioTrimmer from \"./AudioTrimmer.vue\"\n\nexport default {\n  name: \"AudioBox\",\n  components: {\n    AudioTrimmer\n  },\n  mixins: [AudioFunctions,UploadFunctions],\n  props: ['appData','item','mode'],\n  data() {\n    return {\n      status: 'initial',\n      likeUpdated: false,\n      playlistScreen: 0,\n      playlistTitle: 'Enter playlist title'\n    }\n  },\n  computed: {\n    activateEffects() {\n      return this.mode !== 'edit' && this.appData.currentItem._id === this.item._id;\n    },\n    isLiked() {\n      const userLikes = this.$store.state.currentUser.likes != 1 ? this.$store.state.currentUser.likes: [];\n      return userLikes && userLikes.includes(this.item._id);\n    },\n    userLoggedIn() {\n      return (this.$store.state.currentUser._id);\n    },\n    currentlyPlaying() {\n      return (this.appData.currentItem._id === this.item._id);\n    },\n    userPlaylists(){\n      console.log(this.$store.state.currentUser.playlists);\n      return this.$store.state.currentUser.playlists || [];\n    }\n  },\n  methods: {\n    editableUnfocus(event,attribute) {\n      const value = event.target.innerText;\n      this.item.editable[attribute] = value;\n    },\n    updateTrim(trimTime) {\n      this.item.editable.preview.start = trimTime.start;\n      this.item.editable.preview.duration = trimTime.duration;\n    },\n    saveTrim() {\n      const trimData = new FormData();\n        trimData.append('_id', this.item._id);\n        trimData.append('start', this.item.editable.preview.start);\n        trimData.append('duration', this.item.editable.preview.duration);\n        axios.post('/trimAudio', trimData)\n        .then(response => {\n          console.log(response);\n        })\n        .catch(err => {\n          console.log(err);\n        });\n    },\n    toggleEffect(effect) {\n      if (this.item.editable.effects.includes(effect)) {\n        this.item.editable.effects = this.item.editable.effects.filter(e => e !== effect);\n      } else {\n        this.item.editable.effects = [...this.item.editable.effects,effect];\n      }\n    },\n    toggleTag(tagId) {\n      if (this.item.editable.tags.includes(tagId)) {\n        this.item.editable.tags = this.item.editable.tags.filter(e => e !== tagId);\n      } else {\n        this.item.editable.tags = [...this.item.editable.tags,tagId];\n      }\n    },\n    async deleteSound(){\n      const formData = new FormData();\n      formData.append('sound', JSON.stringify(this.item));\n      axios.post('/deleteSound', formData)\n        .then(response => {\n          console.log(response);\n          this.$emit('deleteSound', this.item._id);\n        })\n        .catch(err => {\n          console.log(err);\n        });      \n    },\n\n    // User Interactions\n    updateLike() {\n      this.$data.likeUpdated = true;\n      var soundId = this.item._id;\n      var likeOrUnlike = this.isLiked ? 'unlike': 'like';\n      this.$store.dispatch('updateLike', { \n        likeOrUnlike: likeOrUnlike,\n        soundId:soundId\n      })\n    },\n\n    // Playlists\n    togglePlaylistOptions() {\n      this.playlistScreen = this.playlistScreen === 0 ? 1: 0;\n    },\n    updatePlaylistItem(playlistId) {\n      var soundId = this.item._id;\n      this.$store.dispatch('updatePlaylistItem', { \n        soundId: soundId,\n        playlistId: playlistId \n      })\n    },\n    createPlaylistTitle(event) {\n      const value = event.target.innerText;\n      this.playlistTitle = value;\n    },\n    addPlaylist(){\n      this.playlistScreen = 2;\n    },\n    createPlaylist(){\n      var audioBoxData = this;\n      this.$store.dispatch('createPlaylist', { \n        soundId: this.item._id,\n        playlistTitle: this.playlistTitle,\n        callback: function(){\n          audioBoxData.playlistScreen = 1;\n        } \n      });\n      \n      this.playlistTitle = '';\n    }\n  },\n  created() {\n    this.item.mode = this.mode; // Set item mode\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n  $border-size: 2px;\n  $border-radius: 15px;\n\n  .AudioBox {\n      position: relative;\n      overflow: hidden;\n      border-radius: $border-radius;\n      box-sizing: border-box;  \n      transition: background-color 2s;  \n      background: white;\n      padding: 10px;\n      margin-bottom: 10px;\n      min-height: 150px;\n      &[data-mode=\"upload\"] {\n        padding: 20px;\n      }\n      .AudioBox_Section {\n        margin-bottom: 10px;\n        text-align: left;\n      }\n      .AudioBox_Section, \n      .AudioBox_Row {  \n        margin-top: 0;\n        padding-top: 0; \n        &:first-child {\n          margin-top: 0;\n          padding-top: 0;\n        }\n        &:last-child {\n          margin-bottom: 0;\n          padding-bottom: 0;\n          border-bottom: none;\n        }\n        }\n      .AudioBox_Row ~ .AudioBox_Row {\n        margin-top: 10px;\n      }\n      .AudioBox_Column {\n        display: flex;\n        flex-wrap: nowrap;\n        align-items: flex-start;\n        padding: 2.5px;\n        &:first-child { padding-left: 0; }\n        &:last-child { padding-right: 0; }\n      }\n      .AudioBox_SectionHeader1 {\n        font-size: 14px;\n        font-weight: 600;\n        text-transform: uppercase;\n        margin-bottom: 7.5px;\n      }\n      .AudioBox_SectionHeader2 {\n        font-size: 12px;\n        font-weight: 600;\n        margin-bottom: 2.5px;\n      }\n\n      // Player\n      $playerControlsHeight: 50px;\n      .AudioBox_Player {\n          display: inline-block;\n          vertical-align: middle;\n          height: $playerControlsHeight;\n          font-size: 20px;\n          font-weight: bold;\n          audio { display: none !important; height: 0; width: 0; opacity: 0} // Hide audio element\n          // Controls\n          .AudioBox_PlayerControls {\n              display: flex;\n              flex-direction: row;          \n            .AudioBox_PlayerCube {\n              position:relative;\n              cursor: pointer;\n              height: 50px;\n              line-height: 50px;\n              width: 50px;\n              box-sizing: border-box;\n              background-color: rgba(226, 226, 226, 0.65);\n              border-radius: 10px;\n              text-align: center;\n              font-size: 14px;\n              transition: all 0.05s;\n              &:before {\n                content: '';\n                background-image: url(~@/assets/icons-audioplayer.svg);\n                height:100%;\n                width:100%;\n                position:absolute;\n                top:0;\n                left:0;\n                opacity: 0.5;\n              }\n            }\n            .AudioBox_PlayerWaveform {\n              display: none;\n              height: $playerControlsHeight;\n              flex-grow: 1;\n              box-sizing: border-box;\n              border: $border-size solid black;\n              border-radius: $border-radius;\n            }\n          }\n          margin-right: 10px;\n      }\n\n      // Details\n      .AudioBox_Main {\n        flex-grow: 1;\n      }\n      .AudioBox_Details {\n          display: inline-block;\n          vertical-align: middle;\n          .AudioBox_Title {\n            line-height: 100%;\n            font-size: 18px;\n            font-weight: bold;\n            word-break: break-all;\n            white-space: normal;\n            margin-bottom: 5px;\n          }\n          .AudioBox_Author {\n            font-size: 14px;\n          }\n      }\n      .AudioBox_Description {\n        line-height: 150%;\n        font-size: 12px;\n        word-break: break-all;\n      }\n\n      // Tools\n      .AudioBox_Tools {\n      }\n      .AudioBox_Delete,\n      .AudioBox_Buy {\n        min-width: 50px;\n        padding: 5px;\n        border-radius: 5px;\n        font-weight: 600;\n        font-size: 10px;\n        text-align: center;\n        text-transform: uppercase;\n        .AudioBox_BuyCurrency {\n          margin-right: 1.5px;\n          font-size: 10px;\n          color: rgba(0, 0, 0, 0.75);\n        }\n        .AudioBox_BuyPrice {\n\n        }\n      }\n      .AudioBox_Delete,\n      .AudioBox_Buy {\n          margin-right: 5px;\n      }\n\n      // Settings\n      [contenteditable=\"true\"]  {\n        outline: 0;\n        &:hover:not(:focus) {\n          background: map-get($colors, \"yellow\")\n        }\n      }\n\n      .AudioBox_MarketplacePrice {\n        display: inline-block;\n        padding: 5px 10px;\n        border-radius: 5px;\n        font-size: 16px;\n        color: rgba(0, 0, 0, 0.75);\n        &:before {\n          content: '$';\n        }\n        input {\n          font-size: 16px;\n          font-weight: 600;\n          border: 0;\n          background: transparent;\n          &:focus {\n            outline: none;\n          }\n        }\n      }\n      // State\n      &[data-status=\"initial\"],\n      &[data-status=\"inactive\"] {\n          .AudioBox_PlayerControls{\n            .AudioBox_PlayerCube {\n              &:before {\n                background-image: url(~@/assets/icons-audioplayer.svg);\n              }\n            }\n          }\n      }\n      &[data-status=\"playing\"] {\n          .AudioBox_PlayerControls{\n            .AudioBox_PlayerCube {\n              &:before {\n                background-image: url(~@/assets/icons-audioplayer.svg);\n                background-position: -75px 0;\n              }\n            }\n          }\n      }  \n      &[data-color=\"grey\"] {\n        /* Permalink - use to edit and share this gradient: https://colorzilla.com/gradient-editor/#e2ebf0+0,cfd9df+100 */\n        background: #e2ebf0; /* Old browsers */\n      }\n      &[data-color=\"yellow\"] {\n        background-color: #fff06d;\n      }\n      \n      // Animations\n      &[data-animation*=\"genesis9-cubes\"],\n      &[data-animation*=\"spectrum-lines\"] {\n        .AudioBox_EQ {\n            position: absolute;\n            top: 0;\n            bottom: 0;\n            left: 0;\n            right: 0;\n            display: flex;\n            border-radius: $border-radius;\n            flex-direction: row;\n            transition: all 0.5s;\n            -webkit-transition: all 0.5s;\n    \n            [data-level=\"0\"],[data-level=\"1\"],[data-level=\"2\"] {\n              background: #ff7887;\n            }\n            [data-level=\"2\"],[data-level=\"3\"],[data-level=\"4\"],[data-level=\"5\"],[data-level=\"6\"],[data-level=\"7\"] {\n              background: #ffa371;\n            }\n            [data-level=\"8\"],[data-level=\"9\"],[data-level=\"10\"],[data-level=\"11\"],[data-level=\"12\"],[data-level=\"13\"] {\n              background: #fff06d;\n            }\n            [data-level=\"9\"],[data-level=\"10\"],[data-level=\"11\"],[data-level=\"12\"],[data-level=\"13\"],[data-level=\"14\"] {\n              background: #d8ff83;\n            }   \n            [data-level=\"15\"],[data-level=\"16\"],[data-level=\"17\"],[data-level=\"18\"],[data-level=\"19\"],[data-level=\"20\"] {\n              background: #73c0ff;\n            }\n            [data-level=\"21\"],[data-level=\"22\"],[data-level=\"23\"],[data-level=\"24\"],[data-level=\"25\"],[data-level=\"26\"] {\n              background: #bc88ff;\n            }\n            [data-level=\"27\"],[data-level=\"28\"],[data-level=\"29\"],[data-level=\"30\"],[data-level=\"31\"],[data-level=\"32\"] {\n              background: #ff8be6;\n            }\n        }\n        &.deepBass {\n          -webkit-animation: deepBass 0.05s linear infinite both;\n          animation: deepBass 0.05s linear infinite both;\n        }\n      }\n      &[data-animation*=\"genesis9-cubes\"] .AudioBox_EQ {\n        background-color: #a967ff;\n        flex-wrap: wrap;\n        [data-level] {\n          opacity: 0;\n          flex-grow: 1;\n          min-width: 50px;\n        }\n      }\n      &[data-animation*=\"boombox\"] {\n        &[data-color=\"grey\"] {\n          &.deepBass {\n            background-color:rgb(137, 192, 255);\n          }\n        }\n        &[data-color=\"yellow\"] {\n          &.deepBass {\n              background-color:rgb(255, 124, 84);\n          }\n        }\n        &.treble {\n          .AudioBox_PlayerCube {\n          }\n        }\n        &.deepBass {\n          -webkit-animation: deepBass 0.05s linear infinite both;\n          animation: deepBass 0.05s linear infinite both;\n        }\n      }\n\n      &[data-animation*=\"colors-steps\"] {\n          .AudioBox_EQ {\n            position: absolute;\n            top: 0;\n            bottom: 0;\n            left: 0;\n            right: 0;\n            display: flex;\n            border-radius: $border-radius;\n            flex-direction: row;\n        }\n        &.deepBass {\n          -webkit-animation: deepBass 0.05s linear infinite both;\n          animation: deepBass 0.05s linear infinite both;\n        }\n        &[data-step=\"0\"] {\n          .AudioBox_EQ {\n            background-color: #bc88ff;\n          }\n        }\n        &[data-step=\"1\"] {\n          .AudioBox_EQ {\n            background-color: #73c0ff;\n          }\n        }\n        &[data-step=\"2\"] {\n          .AudioBox_EQ {\n            background-color: #d8ff83;\n          }\n        }\n        &[data-step=\"3\"] {\n          .AudioBox_EQ {\n            background-color: #fff06d;\n          }\n        }\n        &[data-step=\"4\"] {\n          .AudioBox_EQ {\n            background-color: #ffa371;\n          }\n        }\n        &[data-step=\"5\"] {\n          .AudioBox_EQ {\n            background-color: #ff7887;\n          }\n        }\n        &[data-step=\"6\"] {\n          .AudioBox_EQ {\n            background-color: #ff8be6;\n          }\n        }\n      }\n      }\n      @-webkit-keyframes deepBass {\n        0% {\n          -webkit-transform: translate(0);\n                  transform: translate(0);\n        }\n        20% {\n          -webkit-transform: translate(-1.5px, 1.5px);\n                  transform: translate(-1.5px, 1.5px);\n        }\n        40% {\n          -webkit-transform: translate(-1.5px, -2px);\n                  transform: translate(-1.5px, -2px);\n        }\n        60% {\n          -webkit-transform: translate(1.5px, 2px);\n                  transform: translate(1.5px, 2px);\n        }\n        80% {\n          -webkit-transform: translate(1.5px, -1.5px);\n                  transform: translate(1.5px, -1.5px);\n        }\n        100% {\n          -webkit-transform: translate(0);\n                  transform: translate(0);\n        }\n      }\n      @keyframes deepBass {\n        0% {\n          -webkit-transform: translate(0);\n                  transform: translate(0);\n        }\n        20% {\n          -webkit-transform: translate(-1.5px, 1.5px);\n                  transform: translate(-1.5px, 1.5px);\n        }\n        40% {\n          -webkit-transform: translate(-1.5px, -2px);\n                  transform: translate(-1.5px, -2px);\n        }\n        60% {\n          -webkit-transform: translate(1.5px, 2px);\n                  transform: translate(1.5px, 2px);\n        }\n        80% {\n          -webkit-transform: translate(1.5px, -1.5px);\n                  transform: translate(1.5px, -1.5px);\n        }\n        100% {\n          -webkit-transform: translate(0);\n                  transform: translate(0);\n        }\n      }\n      @-webkit-keyframes crazyShake {\n        0% {\n          -webkit-transform: translate(0);\n                  transform: translate(0);\n        }\n        20% {\n          -webkit-transform: translate(2px, -3.5);\n                  transform: translate(3.5, -3.5);\n        }\n        40% {\n          -webkit-transform: translate(3.5, 3.5);\n                  transform: translate(3.5, 3.5);\n        }\n        60% {\n          -webkit-transform: translate(-3.5, 3.5);\n                  transform: translate(-3.5, 3.5);\n        }\n        80% {\n          -webkit-transform: translate(-3.5, -3.5);\n                  transform: translate(-3.5, -3.5);\n        }\n        100% {\n          -webkit-transform: translate(0);\n                  transform: translate(0);\n        }\n      }\n      @keyframes crazyShake {\n        0% {\n          -webkit-transform: translate(0);\n                  transform: translate(0);\n        }\n        20% {\n          -webkit-transform: translate(3.5px, -3.5px);\n                  transform: translate(3.5px, -3.5px);\n        }\n        40% {\n          -webkit-transform: translate(3.5px, 3.5px);\n                  transform: translate(3.5px, 3.5px);\n        }\n        60% {\n          -webkit-transform: translate(-3.5px, 3.5px);\n                  transform: translate(-3.5px, 3.5px);\n        }\n        80% {\n          -webkit-transform: translate(-3.5px, -3.5px);\n                  transform: translate(-3.5px, -3.5px);\n        }\n        100% {\n          -webkit-transform: translate(0);\n                  transform: translate(0);\n        }\n      }\n\n     .VisualEffects_Selection {\n        [data-effect] {\n          overflow: hidden;\n          cursor: pointer;\n          display: inline-block;\n          vertical-align: top;\n          border-radius: 5px;\n          background: rgb(206, 206, 206);\n          margin-right: 5px;\n          height: 45px;\n          width: 45px;\n\n          &.currentSelection {\n              transform: scale(1.05);\n              box-sizing: border-box;\n              box-shadow: 0 1px 1px rgba(0,0,0,0.05), \n              0 2px 2px rgba(0,0,0,0.05), \n              0 4px 4px rgba(0,0,0,0.05), \n              0 6px 8px rgba(0,0,0,0.05),\n              0 8px 16px rgba(0,0,0,0.05);\n          }\n        }\n\n        [data-effect=\"genesis9-cubes\"] {\n          background-color: #a967ff;\n          div {\n            float: left;\n            width: 33.34%;\n            height: 33.34%;\n            \n            @each $colorName, $colorHex in $colors {\n              &[data-background=\"#{$colorName}\"] { \n                animation: blink ((random(10)+1) * 0.1s) infinite;                \n              }\n            }\n          }\n        }\n\n        @keyframes blink {\n          0%,\n          100% {\n            opacity: 1;\n          }\n          50%{\n            opacity: 0;\n          }\n        }\n\n        [data-effect=\"colors-steps\"] {\n          animation: colors-steps 5s infinite;\n        }\n        @keyframes colors-steps {\n          0%   {background-color: map-get($colors, \"red\");}\n          16.65%   {background-color: map-get($colors, \"red\");}\n\n          16.66%  {background-color: map-get($colors, \"orange\");}\n          33.32%  {background-color: map-get($colors, \"orange\");}\n\n          33.33%  {background-color: map-get($colors, \"yellow\");}\n          49.98%  {background-color: map-get($colors, \"yellow\");}\n\n          49.99%  {background-color: map-get($colors, \"green\");}\n          66.65%  {background-color: map-get($colors, \"green\");}\n\n          66.66%  {background-color: map-get($colors, \"blue\");}\n          83.32%  {background-color: map-get($colors, \"blue\");}\n\n          83.33%  {background-color: map-get($colors, \"indigo\");}\n          99.99%  {background-color: map-get($colors, \"indigo\");}\n\n          100% {background-color: map-get($colors, \"red\");}\n        }\n      }\n\n      .UserInteraction {\n        display: inline-block;\n    