\"(route, index) in navRoutes\" :key=\"index\" :to=\"route.path\">{{route.name}}</router-link>\n    </div>\n</template>\n\n<script>\nexport default {\n  name: \"Navigation\",\n  props: ['appData'],\n  computed: {\n    navRoutes() {      \n      const routes = this.appData.routes.filter(route => {\n        const authenticated = \n          this.$store.state.currentUser._id &&\n          this.$store.state.currentUser.status === 'active' \n          ;\n        const auth = \n          !authenticated && route.meta && route.meta.requiresAuth === true ? false: \n          authenticated && route.meta && route.meta.requiresNoAuth === true ? false:\n          true;\n          \n        return route.nav === true & auth === true;\n      });\n      return routes;\n    }\n  },\n  methods: {\n    \n  },\n  mounted() {\n    \n  }\n};\n</script>\n\n<style lang=\"scss\" scoped>\n  .PageLink {\n    display: inline-block;\n     font-size: 0.875rem;\n     font-weight: 600;\n     color: rgb(41, 41, 41); \n     text-decoration: none;\n     text-transform: uppercase;\n     &:not(:last-child){\n       margin-right: 20px;\n     }\n  }\n</style>\n"],"sourceRoot":"src/components/structure"}]}