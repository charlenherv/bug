om './components/structure/ControlBottom.vue'\n\n// Tools\nimport SuperInput from './components/general/SuperInput.vue'\n\nimport StayAwake from 'stayawake.js';\nStayAwake.init();\n\nimport routes from './components/pages/routes.js'\n\nexport default {\n  name: 'App',\n  components: {\n    SuperInput,\n    ControlTop,\n    ControlBottom\n  },\n  methods: { \n    mouseMove(e) {\n      this.globalAppData.shared.mousePosition = {\n          top: e.pageY,\n          left: e.pageX\n      }\n    }\n  },\n  data(){\n    return {\n      test: {},\n      globalAppData: {\n        routes: routes,\n        shared: {\n          mousePosition: {\n            top: 0,\n            left: 0\n          }\n        },\n        tools: {\n          superInput: {\n            state: false\n          }\n        },\n        currentItem: {\n        },\n        mainItems: [],\n        featuredItems: [\n          {\n            id:'000'\n          },\n          {\n            id:'003'\n          },\n          {\n            id:'006'\n          }\n        ],\n        messages: {\n          siteWide: [\n             /* {\n              header:\"Beta\",\n              text:'This site is still under development. Use at your own risk.',\n              color: '#777777'\n            }*/\n          ],\n          userRelated: [\n           /* {\n              header:\"Profile 10% Complete\",\n              text:'Update your profile for bonus credits.',\n              color: '#777777'\n            }*/\n          ]\n          \n        },\n        effects: {\n          power: true\n        },\n        tags: [],\n        itemSettings: {\n          marketplace: {\n            mode: [\n              {\n                'id': 'none',\n                'label': 'None',\n                'color': 'blue'\n              },\n              {\n                'id': 'freeDownload',\n                'label': 'Free Download',\n                'color': 'blue'\n              },\n              {\n                'id': 'forSale',\n                'label': 'For Sale',\n                'color': 'green'\n              }\n            ]\n          },\n          tags: [\n            {\n              id: 'chill',\n              label: 'Chill'\n            },\n            {\n              id: 'lofi',\n              label: 'LoFi'\n            },\n            {\n              id: 'smooth',\n              label: 'Smooth'\n            },\n            {\n              id: 'epic',\n              label: 'Epic'\n            },\n          ]\n        },\n        admin: {\n          developer: {\n            tools: {\n            }\n          }\n        }\n      }\n    }\n  },\n  beforeMount() {\n\n    axios\n      .get('/mainFeed')\n      .then(response => (this.globalAppData.mainItems = response.data.sounds))\n\n  },\n  mounted(){\n    window.addEventListener('mousemove', this.mouseMove);\n  },\n  destroyed: function() {\n    window.removeEventListener('mousemove', this.mouseMove);\n  }}\n</script>\n\n<style lang=\"scss\">\n  // IMPORTS\n  @import url('https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;700&display=swap');\n\n  // ELEMENTS\n  body {\n    margin: 0;\n    padding: 0;\n    font-family: 'Open Sans', sans-serif;\n    font-weight: 400;\n  }\n  #app {\n    overflow: hidden;\n    position: absolute;\n    display: flex;\n    flex-direction: column;\n    top: 0;\n    left: 0;\n    right: 0;\n    bottom: 0;\n  }\n</style>\n"],"sourceRoot":"src"}]}